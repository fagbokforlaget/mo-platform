    "@babel/preset-env": "^7.13.15",
    "@babel/register": "^7.13.16",
    "babel-loader": "^8.2.2",
    "chai": "4.3.4",
    "compression-webpack-plugin": "^7.1.2",
    "core-js": "^3.11.0",
    "eslint": "7.25.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
